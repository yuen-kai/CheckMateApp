{
  "expo": {
    "name": "CheckMate",
    "slug": "CheckMate",
    "version": "2.2.0",
    "orientation": "portrait",
    "icon": "./assets/Icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#6a99e6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/3b3f319e-e798-4109-8eff-d06a7eb8f6b4"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yuenkai.CheckMate",
      "buildNumber": "2.2.0",
      "infoPlist": {
        "NSRemindersUsageDescription": "This app reads events from the device's calendar and adds them into the user's schedule in the app."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AdaptiveIcon.png",
        "backgroundColor": "#6a99e6"
      },
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/Icon.png",
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.yuenkai.CheckMate",
      "versionCode": 11
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/NotificationIcon.png",
          "color": "#6a99e6"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "3b3f319e-e798-4109-8eff-d06a7eb8f6b4"
      }
    }
  }
}
